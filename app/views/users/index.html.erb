<%= render 'books/side', user: @user, book: @book %>

<h3>Users</h3>
<Table>
  <tr>
    <th>image</th>
    <th>name</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <th>
      <%= image_tag user.get_profile_image(100,100) %>
    </th>
    <th>
      <%= user.name %>
    </th>
    <th>
      <%= link_to "Show", user_path(user.id) %>
    </th>
  </tr>
  <% end %>
</Table>
